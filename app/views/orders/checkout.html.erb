<%= form_for @order, url: place_order_order_path(@order.id), method: :post do |f| %>
    <%= f.label 'Address' %>
    <%= f.text_field :address %>
    <%= f.label 'Name' %>
    <%= f.text_field :name %>
    <%= f.label 'Email' %>
    <%= f.text_field :email %>
    <%= f.label 'Phone' %>
    <%= f.text_field :phone %>
    <%= f.submit %>   
<% end %>